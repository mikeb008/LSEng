	view mobileApp of lseng.uiLayer.diagnosticTools.mobileApp {

		global  style diagnosticTools
        global  style interventionTools
        global  style ai-conversationalTools

		group 'Mobile User Interface' {
			color green
    		opacity 40%
    		border solid
			include lseng.uiLayer.diagnosticTools.mobileApp, lseng.uiLayer.diagnosticTools.mobileApp.*
			//include element.tag = #mobileApp
			
		}
		include lseng.uiLayer.mobileApp with {
    		navigateTo mobileApps-l3
  		}
		include lseng.uiLayer.mobileApp with {
    		navigateTo lseng
  		}
	}

	view mobileApps-l3 of lseng.uiLayer.mobileApp {
		include lseng.uiLayer.mobileApp, lseng.uiLayer.mobileApp.*
		
		global  style diagnosticTools
        global  style interventionTools
        global  style ai-conversationalTools

		include lseng.uiLayer.mobileApp with {
    		navigateTo mobileApp
  		}
	}

	
