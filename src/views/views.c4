	view lseng of lseng {
		
		title 'System Context Diagram'
		description 'System Context'
		include lseng, lseng.*, 
			personas, personas.*,
			lseng.uiLayer, lseng.uiLayer.*,
			lseng.appLayer, lseng.appLayer.*, 
			lseng.dataLayer, lseng.dataLayer.*

		exclude *  ->  lseng.dataLayer.db
		exclude lseng.uiLayer.webApp.apiClient -> lseng.middleware 
		
		global  style diagnostic-tools
        global  style interventionTools
        global  style ai-conversational-tools
		
		style  element.tag = #longevity-interventionTools 	{notation "Longevity Intervention Tools"}
		style  element.tag = #longevity-diagnostic-tools   	{notation "Longevity Diagnostic Tools"}
		style  element.tag = #ai-conversational-tools 		{notation "AI Conversational Tools"}

		autoLayout TopBottom
	}

  
  
  
