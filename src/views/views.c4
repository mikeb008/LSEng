	view system-context {

		style element.tag = #longevity-interventionTools {
    		notation "Longevity Intervention Tools"
    		color primary
  		}

		style element.tag = #longevity-diagnostic-tools {
    		notation "Longevity Diagnostic Tools"
    		color sky
  		}
		
		style element.tag = #ai-conversational-tools {
    		notation "AI Conversational Tools'"
    		color indigo
  		}

		group  'Longevity Diagnostic Tools' {
			color primary
    		opacity 40%
    		border solid
			include element.tag = #longevity-interventionTools 
			//{
			// lseng.personas.clinician -> * 'Validates Health Ratings' 
			//}
		}

		group 'Longevity Intervention Tools' {
			color sky
    		opacity 40%
    		border solid
			include element.tag = #longevity-diagnostic-tools
			
		}

		group 'AI Conversational Tools' {
			color indigo
    		opacity 40%
    		border solid
			include element.tag = #ai-conversational-tools
		}

		autoLayout TopBottom
	}

 /**
  * @likec4-generated(v1)
  * 
  * 
  * 
  * 
  */
	dynamic view sys-context {
		include lseng.personas,* where tag = #personas
		include lseng.appLayer.apps.diagnostic-tools, lseng.appLayer.apps.diagnostic-tools.* where tag = #longevity-diagnostic-tools
		include lseng.appLayer.apps.interventionTools, lseng.appLayer.apps.interventionTools.* where tag = #longevity-interventionTools
		include lseng.appLayer.ai.conversational-tools, lseng.appLayer.ai.conversational-tools.* where tag = #ai-conversational-tools
		//include lseng.dataLayer, seng.dataLayer.* where tag = #dataLayer
		
		//lseng.appLayer.apps.diagnostic-tools   -> lseng.appLayer.apps.interventionTools 	'Triggers interventions'
		//lseng.appLayer.apps.diagnostic-tools   -> lseng.dataLayer.blockchain				'Stores immutable ratings'
		//lseng.appLayer.apps.diagnostic-tools   -> lseng.dataLayer.datalake					'Writes raw analyutics'
		//lseng.appLayer.apps.interventionTools -> lseng.appLayer.ai.conversational-tools 	'Enhances engagement'
		//lseng.appLayer.apps.ai.conversational-tools -> lseng.seng.personas 					'provide real-time guidance'
		//lseng.personas.clinician 	  -> lseng.appLayer.apps.diagnostic-tools 					'Validates health ratings'
		//lseng.personas.actuary 	  -> lseng.appLayer.apps.diagnostic-tools 					'Uses LOE for Risk Models'
		//lseng.personas.partricipant -> lseng.appLayer.apps.diagnostic-tools 					'Submits Data'
		//lseng.personas.coach 		  -> lseng.appLayer.apps.diagnostic-tools 					'Provides guidance'


		
	}

	

	view mobileApp of lseng.uiLayer.mobileApp {

		group 'Mobile User Interface' {
			color blue
    		opacity 40%
    		border solid
			include lseng.uiLayer.mobileApp
			include element.tag = #mobileApp
			
		}
		include lseng.uiLayer.mobileApp with {
    		navigateTo mobileApps-l3
  		}
		include lseng.uiLayer.mobileApp with {
    		navigateTo lseng
  		}
	}

	view mobileApps-l3 of lseng.uiLayer.mobileApp {
		include lseng.uiLayer.mobileApp, lseng.uiLayer.mobileApp.*
		include lseng.uiLayer.mobileApp with {
    		navigateTo mobileApp
  		}
	}

	view ai of lseng.appLayer.ai {

		group 'AI Conversational Tools' {
			color indigo
    		opacity 40%
    		border solid
			include lseng.appLayer.ai
			include element.tag = #ai-conversational-tools
			exclude lseng.appLayer.algos
		}
		style * {
    		color indigo
  		}
		include lseng.appLayer.ai  with {
    		navigateTo lseng
  		}
	}

	view algos of lseng.appLayer.algos {

		group 'Algorithms' {
			color indigo
    		opacity 40%
    		border solid
			include lseng.appLayer.algos 
			include element.tag = #algorithms
			
		}
		style * {
    		color indigo
  		}
		include lseng.appLayer.algos  with {
    		navigateTo lseng
  		}
	}

	view core-apps of lseng.appLayer.apps {

		group 'Core Apps' {
			//color sky
    		opacity 30%
    		border solid
			include lseng.appLayer.apps, lseng.appLayer.apps.*, 
			        lseng.appLayer.apps.diagnostic-tools.*, lseng.appLayer.apps.interventionTools.*,
			//include * where tag is not #algorithms and tag is not #ai-conversational-tools
			
		}

		style lseng.appLayer.apps.diagnostic-tools {
    		color green
			opacity 50%
  		}
		style lseng.appLayer.apps.interventionTools {
    		color amber
			opacity 30%
  		}


		include lseng.appLayer.apps  with {
    		navigateTo lseng
  		}

		global  style diagnostic-tools
        global  style interventionTools
        global  style ai-conversational-tools
	}
}